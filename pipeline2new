pipeline{
  agent any
    stages {
      stage('build'){
        steps {
          bat 'whoami'
        }
      }
    }
    post{
      always{
        echo 'ALWAYS'
      }
      success {
        echo 'ONLY ON SUCCESS'
      }

      failure {
      echo 'ONLY ON FAILURE'
      }
      unstable {
        echo 'ONLY IF RUN IS UNSTABLE'
      }
      changed {
        echo 'ONLY IS STATUS CHANGED FROM SUCCESS TO FAILURE OR VICE VERSA'
      }

    }
}
