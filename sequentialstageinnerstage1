  agent any
  stages{
    stage('stage1'){
      stages{
        stage('inner stage1'){
          steps{
            echo 'inner stage1'
          }
        }
      }
    }
    stage('stage2'){
      steps{
        echo 'stage 2'
      }
    }
    stage('stage3'){
      steps{
        echo 'stage 3'
      }
    }
  }
}
